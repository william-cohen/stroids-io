 = true;\\n            }\\n            //Right Arrow Key\\n            if (this.keys.isPressed(39)) {\\n                this.rotation += 3 * delta;\\n                input.D = true;\\n            }\\n            //Up Arrow Key\\n            if (this.keys.isPressed(38) || this.keys.isPressed(32)) {\\n                input.W = true;\\n                this.thrusting = true;\\n                thrust.x = 30.0 * Math.cos(this.rotation);\\n                thrust.y = 30.0 * Math.sin(this.rotation);\\n            }\\n            if (input.A || input.D || input.W) {\\n                this.inputNo++; this.inputNo %= 255;\\n                this.socket.emit('input', {keys: input, t: this.inputNo});\\n            }\\n\\n            let drag = this.vel.scale(2.75);\\n            this.vel = this.vel.add(thrust.subtract(drag).scale(delta));\\n\\n            this.pos = this.pos.add(this.vel);\\n        }\\n\\n    }\\n\\n    draw(ctx) {\\n        if (!this.alive) return;\\n        ctx.save();\\n        ctx.translate(this.pos.x, this.pos.y);\\n        ctx.rotate(this.rotation + Math.PI / 2);\\n        ctx.translate(-this.pos.x, -this.pos.y);\\n        if (this.thrusting) {\\n            if (this.tick%10 > 5) {\\n                ctx.drawImage(this.spriteT, this.pos.x - 19, this.pos.y - 24);\\n            } else {\\n                ctx.drawImage(this.spriteT2, this.pos.x - 19, this.pos.y - 24);\\n            }\\n        } else {\\n            ctx.drawImage(this.sprite, this.pos.x - 19, this.pos.y - 24);\\n        }\\n        ctx.restore();\\n    }\\n}\\n\\nmodule.exports = Player;\\n\",\"class KeyListener {\\n    constructor() {\\n        this.pressedKeys = [];\\n        document.addEventListener(\\\"keydown\\\", this.keydown.bind(this));\\n        document.addEventListener(\\\"keyup\\\", this.keyup.bind(this));\\n    }\\n\\n    keydown(e) {\\n        this.pressedKeys[e.keyCode] = true;\\n    }\\n\\n    keyup(e) {\\n        this.pressedKeys[e.keyCode] = false;\\n    }\\n\\n    isPressed(